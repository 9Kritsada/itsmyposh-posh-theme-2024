{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
	"blocks": [
		{
			"alignment": "left",
			"newline": false,
			"segments": [
				{
					"foreground": "#ff5959",
					"properties": {
					  "macos": "\uf179 ",
					  "ubuntu": "\uf31b ",
					  "windows": "\ufd23 "
					},
					"style": "diamond",
					"template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}}",
					"type": "os"
				},
				{
					"foreground": "#ff5959",
					"style": "plain",
					"template": "{{ .HostName }} ",
					"type": "session"
				},
				{
					"foreground": "#cccccc",
					"properties": {
						"style": "folder" 
					},
					"style": "plain",
					"template": "<#cccccc>\uf1fa</> {{ .Path }} ",
					"type": "path"
				},
		
				{
					"foreground": "#1ddd33",
					"properties": {
						"branch_icon": "\uf09b "
					},
					"style": "plain",
					"template": "<#000></>{{ .HEAD }}<#000> </>",
					"type": "git"
				}
			],
			"type": "prompt"
		},
		{
			"alignment": "left",
			"segments": [
				{
					"foreground": "#1ddd33",
					"properties": {
						"always_enabled": true
					},
					"style": "plain",
					"template": "\uf155",
					"type": "text"
				}
			],
			"type": "prompt"
		}
	],
	"final_space": true,
	"version": 2
}
